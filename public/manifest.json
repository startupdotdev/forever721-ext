{
  "manifest_version": 2,
  "name": "Forever721",
  "version": "1.0.0",
  "permissions": ["storage", "activeTab", "webNavigation"],
  "background": {
    "scripts": ["background.js", "hot-reload.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "run_at": "document_idle",
      "js": ["injection.js"]
    }
  ]
}
